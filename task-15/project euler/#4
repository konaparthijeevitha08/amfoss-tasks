a=int(input())

for i in range(a):
    b=int(input())
    c=[]
    for i in range(100,999):
        for j in range(100,999):
            d=i*j
            c.append(d)
    
    c.sort()
    c.reverse()
    
    for i in c:
        j=str(i)
        c.append(j)
        
    f=[]
       
    for j in c:
        if j==j[::-1] :
            f.append(j)
    
    for i in f:
        j=int(i)
        f.append(j)
        
    for i in f:
        if i<=b:
            print(i,"\n")
            break
